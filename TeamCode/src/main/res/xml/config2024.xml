<?xml version="1.0" encoding="utf-8"?>
<Robot type="FirstInspires-FTC">
    <LynxUsbDevice name="Control Hub Portal" serialNumber="(embedded)" parentModuleAddress="173">
        <LynxModule name="Control Hub" port="173">
            <goBILDASeriesMotor name="left_front" port="0" />
            <goBILDASeriesMotor name="left_back" port="1" />
            <goBILDASeriesMotor name="right_back" port="2" />
            <goBILDASeriesMotor name="right_front" port="3" />

            <SparkFunOTOS name="sensor_otos" port="3" bus="1" />
            <RevColorSensorV3 name="sampleColor" port="2" bus="0"/>
            <DistanceSensor name="liftSensor" port="1" bus="0"/>

            <Servo name="intakeExtend" port="0" bus="0" />
            <Servo name="intakeWrist" port="0" bus="2" />
            <CRServo name="sampleServo" port="0" bus="4" />

        </LynxModule>
        <LynxModule name="Expansion Hub 2" port="2">
            <goBILDASeriesMotor name="rightLift" port="0" />
            <goBILDASeriesMotor name="leftLift" port="2" />
            <goBILDASeriesMotor name="ascend" port="1"/>

            <Servo name="specimenServo" port="0" bus="0" />
            <Servo name="ascendServo1" port="0" bus="2" />
            <RevBlinkinLedDriver name="blinkin" port="0" bus="3" />
            <Servo name="ascendServo2" port="0" bus="4" />

        </LynxModule>
    </LynxUsbDevice>\>
</Robot>