<?xml version="1.0" encoding="utf-8"?>
<Robot type="FirstInspires-FTC">
    <LynxUsbDevice name="Control Hub Portal" serialNumber="(embedded)" parentModuleAddress="173">
        <LynxModule name="Control Hub" port="173">
            <goBILDA5202SeriesMotor name="leftFront" port="0" />
            <goBILDA5202SeriesMotor name="leftBack" port="1" />
            <goBILDA5202SeriesMotor name="rightBack" port="2" />
            <goBILDA5202SeriesMotor name="rightFront" port="3" />

            <Servo name="intakeWrist" port="1" />
            <Servo name="bucketServo" port="2" />
            <ContinuousRotationServo name="sampleServo" port="3" />
            <Servo name="intakeExtend" port="4" />
            <Servo name="specimenServo" port="5" />

            <LynxEmbeddedIMU name="imu" port="0" bus="0" />
            <RevColorSensorV3 name="sampleColor" port="0" bus="2"/>
            <SparkFunOTOS2 name="sensor_otos" port="0" bus="3" />
            <DigitalDevice name="liftSensor" port="0"/>

        </LynxModule>
        <LynxModule name="Expansion Hub 2" port = "2" >
            <goBILDA5202SeriesMotor name="rightLift" port="1" />
            <goBILDA5202SeriesMotor name="ascend" port="3"/>
            <goBILDA5202SeriesMotor name="leftLift" port="2" />
            <goBILDA5202SeriesMotor name="ascendLift" port="0" />

            <RevBlinkinLedDriver name="blinkin" port="3" />
        </LynxModule>
    </LynxUsbDevice>
</Robot>